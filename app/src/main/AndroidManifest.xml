<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.madcamp_project2java">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/madwork_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/madwork_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.MadCamp_Project2java">
        <activity android:name=".JoinGroup"></activity>
        <activity android:name=".SignupActivity" />
        <activity android:name=".LoginActivity_retrofit">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>